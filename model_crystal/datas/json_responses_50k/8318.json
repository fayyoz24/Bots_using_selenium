        {
            "title": "Guest Speaker for Master Class on the Euro Crisis",
            "date": {
                "start": {
                    "month": 3,
                    "day": null,
                    "year": 2015
                },
                "end": {
                    "month": 3,
                    "day": null,
                    "year": 2015
