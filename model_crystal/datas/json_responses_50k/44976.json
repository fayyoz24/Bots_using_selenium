            "company": {
                "id": null,
                "name": "Nord Anglia Education",
                "logo": "https://media.licdn.com/dms/image/C4D0BAQGkw_Ymksoguw/company-logo_400_400/0/1519921280394?e=1703721600&v=beta&t=Frfn4C54mxISY6M4xhdh5462lyrlB_s_fiAXY8nshRY",
