        {
            "title": "Cultural Competence in Urban Affairs and Planning",
            "date": {
                "start": {
                    "month": 9,
                    "day": null,
                    "year": 2011
                },
                "end": {
                    "month": 9,
                    "day": null,
                    "year": 2013
