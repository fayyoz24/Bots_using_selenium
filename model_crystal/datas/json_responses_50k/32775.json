        {
            "title": "Dynamic Response Analysis of Golden Gate Bridge",
            "date": {
                "start": {
                    "month": 3,
                    "day": null,
                    "year": 2016
                },
                "end": {
                    "month": 7,
                    "day": null,
                    "year": 2016
