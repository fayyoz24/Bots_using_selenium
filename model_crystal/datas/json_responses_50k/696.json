    "projects": [
        {
            "title": "Migration and Inclusion: chances and challenges for the future",
            "date": {
                "start": {
                    "month": 5,
                    "day": null,
                    "year": 2016
                },
                "end": {
                    "month": null,
                    "day": null,
                    "year": null
                }
            },
            "description": "This double training course consists of two eight-day training events on the topic of migration and inclusion in Berlin and Amsterdam. They will be connected by a five-month try-out and evaluation period of conducting nonformal educational methods and best practice in all partner countries. The participants will learn, try out, discuss and evaluate a large variety of pedagogical approaches to deal with the topics of migration, anti-racist education, stereotypes, intercultural dialogue and conflict management. The trainings involve activities such as doing excursions and guided tours, talking to contemporary witnesses, work with creative and artistic methods, try out methods of anti-racist and trans-cultural education and sharing knowledge and experiences.",
            "contributors": [
                {
                    "type": "standardizedContributor",
                    "first_name": "Revijara",
                    "last_name": "Oosterhuis",
                    "name": null,
                    "headline": "Projectmanager and Coordinator Stichting LOS - Meldpunt Vreemdelingendetentie"
