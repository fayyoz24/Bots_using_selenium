        {
            "title": "Regionalism Redivivus: Do We Need a Closer Look?",
            "date": {
                "start": {
                    "month": 6,
                    "day": null,
                    "year": 2012
                },
                "end": {
                    "month": 6,
                    "day": null,
                    "year": 2012
                }
