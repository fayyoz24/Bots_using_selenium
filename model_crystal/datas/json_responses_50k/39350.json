    "projects": [
        {
            "title": "S\u00e9curisation des droits fonciers et \u00e9tat civil",
            "date": {
                "start": {
                    "month": 1,
                    "day": null,
                    "year": 2014
                },
                "end": {
                    "month": 1,
                    "day": null,
                    "year": 2014
