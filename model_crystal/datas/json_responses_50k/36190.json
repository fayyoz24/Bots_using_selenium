    "projects": [
        {
            "title": "An exploratory Study on Feelings of Safety in Environments using Affective Priming",
            "date": {
                "start": {
                    "month": 9,
                    "day": null,
                    "year": 2011
                },
                "end": {
                    "month": 5,
                    "day": null,
                    "year": 2012
                }
            },
            "description": "In this research project, we aimed at investigating whether the perception of safety in nocturnal urban environments was an affective process, or a cognitive process. We applied an affective priming paradigm in the form of a Lexical Decision Task in which participants were primed with validated images of nocturnal urban settings.",
            "contributors": [
