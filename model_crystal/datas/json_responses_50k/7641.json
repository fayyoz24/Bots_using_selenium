        {
            "title": "Projectgroep bidbook Nationaal Park Hollandse Duinen",
            "date": {
                "start": {
                    "month": 6,
                    "day": null,
                    "year": 2016
                },
                "end": {
                    "month": 10,
                    "day": null,
                    "year": 2016
