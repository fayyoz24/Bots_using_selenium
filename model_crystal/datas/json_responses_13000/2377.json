        {
            "title": "FP7 HOMBRE Holistic Management of BRownfield REgeneration",
            "date": {
                "start": {
                    "month": 12,
                    "day": null,
                    "year": 2010
                },
                "end": {
                    "month": 12,
                    "day": null,
                    "year": 2010
